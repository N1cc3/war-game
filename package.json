{
  "name": "war-game",
  "version": "2019.1",
  "repository": "git@github.com:N1cc3/war-game.git",
  "author": "Niclas Lindgren <niclaslindgren91@hotmail.com>",
  "contributors": [
    "Robert Ekholm <robert@ekholmgroup.fi>"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "prettier --write",
      "git add",
      "yarn test --findRelatedTests"
    ]
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/express": "4.17.2",
    "@types/jest": "24.0.24",
    "@types/mongoose": "5.5.34",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "3.7.3"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "mongoose": "5.8.2"
  }
}
